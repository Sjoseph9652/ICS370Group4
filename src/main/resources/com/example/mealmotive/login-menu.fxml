            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="226.0" layoutY="138.0" />
      <TextField fx:id="passwordTextField" layoutX="226.0" layoutY="212.0" />
      <Label layoutX="158.0" layoutY="142.0" text="Username" />
      <Label layoutX="160.0" layoutY="216.0" text="Password" />
      <Button fx:id="submitButton" layoutX="263.0" layoutY="265.0" mnemonicParsing="false" onAction="#onSubmitButtonClick" text="Submit" />
